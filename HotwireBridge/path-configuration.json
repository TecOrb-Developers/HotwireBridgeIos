{
  "settings": {},
  "rules": [
    {
        "patterns": [".*/welcome"],
        "properties": {
            "view_controller" : "welcome"
        }
    },
    {
        "patterns": [".*/profile"],
        "properties": {
            "view_controller" : "profile"
        }
    },
    {
      "patterns": [
        ".*"
      ],
      "properties": {
        "context": "default",
        "pull_to_refresh_enabled": true
      }
    },
    {
      "patterns": [
        "/new$"
      ],
      "properties": {
        "context": "default",
        "pull_to_refresh_enabled": false
      }
    }
  ]
}
